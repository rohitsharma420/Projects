
package trimtosizemethod;

import java.util.ArrayList;


 /* @author RoHiT
 */
public class TrimtoSizeMethod {

   
    public static void main(String[] args) {
       ArrayList<Integer>obj=new ArrayList<>(20);
       
       obj.add(1);
       obj.add(2);
       obj.add(3);
       obj.add(4);
       obj.add(5);
        // We have a list of five elements but we have the size of array(20), now trim it to actual size to save some memory
        
        obj.trimToSize();
        
        System.out.println(obj);
    }
    
}
