
package sizemethod;
import java.util.ArrayList;
 /* @author RoHiT
 */
public class SizeMethod {


    public static void main(String[] args) {
      ArrayList<Integer>obj=new ArrayList<>();
      
      obj.add(1);
      obj.add(2);
      obj.add(3);
      obj.add(4);
      obj.add(5);
      
        System.out.println("The Size of the arrayList is:"+obj.size());
    }
    
}
