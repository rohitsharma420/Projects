
package addallmethod;

import java.util.ArrayList;

 /* @author RoHiT
 */
public class AddAllMethod {

    
    public static void main(String[] args) {
      ArrayList<String>obj=new ArrayList<>();
      
      obj.add("AA");
      obj.add("BB");
      obj.add("CC");
      
        System.out.println("Element in 1st ArrayList:"+obj);
        
      ArrayList<String>str=new ArrayList<>();
      
      str.add("Rohit");
      str.add("Ravish");
      str.add("Ajay");
      
        System.out.println("Elements in Second ArrayList:"+str);
        
        // Now adding all the elements of of both arraylist into One
        
        obj.addAll(str);
        System.out.println("Elements of Both ArrayList:"+obj);
    }
    
}
